#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/node_modules:/mnt/c/Users/Olamide.thinker/Documents/node_modules:/mnt/c/Users/Olamide.thinker/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/typescript@4.9.5/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules/.pnpm/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/syncsalez-backend-repo/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/syncsalez-mobile/node_modules:/mnt/c/Users/Olamide.thinker/Documents/syncsalez-works/node_modules:/mnt/c/Users/Olamide.thinker/Documents/node_modules:/mnt/c/Users/Olamide.thinker/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
